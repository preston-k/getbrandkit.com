'use strict'

const { exec } = require('child_process')

task('obfuscate', () => {
  exec('npx javascript-obfuscator src --output src-obfuscated --config config-obfuscator.json', (err, stdout, stderr) => {
    if (err) {
      console.error(`Error: ${stderr}`)
    } else {
      console.log(`Output: ${stdout}`)
    }
  })
})